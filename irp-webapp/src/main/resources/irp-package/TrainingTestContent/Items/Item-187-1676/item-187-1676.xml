    <content language="ENU" version="2.0" approvedVersion="6">
      <rationaleoptlist />
      <stem><![CDATA[<p style="">The height of adult women in the United States can be approximated by a normal distribution with a mean of 65 inches and a standard deviation of 3 inches. </p><p style="">&#xA0;</p><p style=""><span id="item_1676_TAG_2" class="its-tag" data-tag="word" data-tag-boundary="start" data-word-index="1"></span>Click<span class="its-tag" data-tag-ref="item_1676_TAG_2" data-tag-boundary="end"></span> on the number line <span id="item_1676_TAG_9" class="its-tag" data-tag="word" data-tag-boundary="start" data-word-index="2"></span>to show<span class="its-tag" data-tag-ref="item_1676_TAG_9" data-tag-boundary="end"></span> a vertical line that approximates the <span id="item_1676_TAG_11" class="its-tag" data-tag="word" data-tag-boundary="start" data-word-index="4"></span>height<span class="its-tag" data-tag-ref="item_1676_TAG_11" data-tag-boundary="end"></span> <span id="item_1676_TAG_10" class="its-tag" data-tag="word" data-tag-boundary="start" data-word-index="3"></span>at which<span class="its-tag" data-tag-ref="item_1676_TAG_10" data-tag-boundary="end"></span> <span id="item_1676_TAG_7_BEGIN">25%</span> of the women are shorter and <span id="item_1676_TAG_8_BEGIN">75%</span> are taller.</p>]]></stem>
      <rubriclist>
        <rubric scorepoint="">
          <name>
        Rubric </name>
          <val><![CDATA[<p style="">Exemplar:</p><p style="">&#xA0;</p><ul style="padding-left:1.5000em; "><li><p style="">The student should draw a vertical line at a height of 63 inches. </p></li></ul><p style="">&#xA0;</p><p style="">For this item, a full-credit response includes</p><p style="">&#xA0;</p><ul style="padding-left:1.5000em; "><li><p style="background-color:transparent; ">a single vertical line that intersects the<span style="font-style:italic; "> x</span>-axis at 63 inches. (1 point)</p><p style="">&#xA0;</p></li></ul>]]></val>
        </rubric>
